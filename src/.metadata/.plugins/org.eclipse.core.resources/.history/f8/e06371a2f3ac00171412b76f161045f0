package ca.jay.jac444.assignment1;

import java.util.Date;

public class TestAssinment1Task1 {

	public static final String PREMIUM = "PREMIUM";
	public static final String GOLD    = "GOLD";
	public static final String SILVER  = "SILVER";		
	
	public static void main(String[] args) {
		try
		{
			Visit[] visits = { 
					new Visit ("Lee", new Date()),
					new Visit ("Hong", new Date()),				
					new Visit ("KIm", new Date()),
					new Visit ("Jung", new Date())					
			};
			visits[0].customer.setMember(true);
			visits[0].customer.setMemberType(PREMIUM);
			
			
			
		}
		catch(Exception e)
		{
			System.out.println(e.toString());
			
		}
	}

}
