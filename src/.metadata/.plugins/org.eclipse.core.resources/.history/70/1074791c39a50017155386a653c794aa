public class Meeting {

	public static void main(String[] args)
	{
		Student s1 = new Student("John", "Smith", "J@gamil.com", "CPD");
		Student s2 = new Student("John", "Smith Jr", "J@gamil.com", "CPD");
		
		Person[] working = new Person[2];
		
		working[0] = s1;
		working[1] = s2;
		
		
		
	}
	
	
	
